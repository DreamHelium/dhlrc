#: src/main.c:78
#, c-format
msgid "Usage: %s [file]\n"
msgstr "用法：%s [文件]\n"

#: src/main.c:94
msgid "Error when reading file."
msgstr "在读取文件时出错。"

#: src/main.c:103
msgid "Not a valid NBT file!"
msgstr "不是一个有效的NBT文件！"

#: src/main.c:120
msgid "Valid NBT file!"
msgstr "有效的NBT文件！"

#: src/main.c:130
#, c-format
msgid ""
"There are three functions:\n"
"[0] NBT lite reader with modifier\n"
"[1] Litematica material list with recipe combination\n"
"[2] Litematica block reader\n"
"\n"
msgstr ""
"有三个功能可供使用：\n"
"[0] 小型NBT阅读器带修改器\n"
"[1] 投影材料列表及合成表合成功能\n"
"[2] 投影方块阅读器\n"
"\n"

#: src/main.c:134
#, c-format
msgid "Please select an option, or enter 'q' to exit the program (q): "
msgstr "请选择一个选项，或输入 “q” 退出程序 (q): "

#: src/main.c:191 src/main.c:229
#, c-format
msgid "There are %d regions:\n"
msgstr "有 %d 个区域：\n"

#: src/main.c:203
#, c-format
msgid "Processing: region %d / %d : [%ld] %s \n"
msgstr "处理区域 %d / %d : [%ld] %s 中……\n"

#: src/main.c:235
#, c-format
msgid "Enter the region number, or enter 'q' to exit program (q): "
msgstr "输入区域号，或输入 “q” 退出程序 (q)： "

#: src/main.c:248
#, c-format
msgid ""
"You are reading region: [%2d] %s:\n"
"The size of the region is (%d, %d, %d).\n"
"\n"
msgstr ""
"你正在阅读区域 [%2d] %s：\n"
"这个区域的大小为 (%d, %d, %d)。\n"
"\n"

#: src/main.c:251
#, c-format
msgid ""
"Please enter the coordination of the block (just numbers of x y z without "
"additional character\n"
"or enter 'b' to choose another region, enter 'q' to exit the program (b): "
msgstr ""
"请输入方块坐标（仅输入数字，不需要额外字符）\n"
"或输入 “b” 选择其他区域，输入 “q” 退出程序 (b): "

#: src/main.c:320
#, c-format
msgid "The block in (%ld, %ld, %ld) is %s.\n"
msgstr "在 (%ld, %ld, %ld) 处的方块是 %s 。\n"

#: src/main.c:321
#, c-format
msgid ""
"Please enter the coordination of the block again, or enter 'b' to choose "
"another region, enter 'q' to exit the program (b): "
msgstr "请再次输入方块的坐标，或输入 “b” 选择其他区域，输入 “q” 退出程序 (b): "

#: src/nbt_litereader.c:181
#, c-format
msgid ""
"The detail of NBT \"%s\" is listed below:\n"
"\n"
msgstr ""

